{
  "name": "staticify",
  "version": "2.0.0",
  "description": "A better static asset handler for Node.js / express.js",
  "main": "index.js",
  "author": "Rakesh Pai <rakeshpai@errorception.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/errorception/staticify.git"
  },
  "scripts": {
    "mocha": "mocha",
    "xo": "xo",
    "test": "npm run xo && npm run mocha"
  },
  "keywords": [
    "static",
    "asset",
    "versioning",
    "caching",
    "express",
    "expressjs"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "ignore-by-default": "^1.0.1",
    "send": "^0.16.2"
  },
  "devDependencies": {
    "mocha": "^5.1.0",
    "should": "^13.2.1",
    "xo": "^0.20.3"
  },
  "xo": {
    "space": 4,
    "rules": {
      "capitalized-comments": "off",
      "spaced-comment": "off",
      "func-style": [
        "error",
        "expression"
      ]
    },
    "overrides": [
      {
        "files": "test/**",
        "envs": [
          "mocha"
        ]
      }
    ]
  },
  "engines": {
    "node": ">=4"
  }
}
